<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.incube.sleuth.UIController">
    <center>
        <VBox alignment="CENTER" spacing="20.0">
            <!--            <Insets top="20" right="20" bottom="20" left="20"/>-->
            <Button fx:id="btnParse" text="Parse" onAction="#handleParseAction"
                    style="-fx-font-family: 'SF Pro Display'; -fx-font-size: 14pt; -fx-background-color: #333; -fx-text-fill: white; -fx-padding: 10;">
                <VBox.margin>
                    <Insets top="20" bottom="20" left="50" right="50"/>
                </VBox.margin>
            </Button>
        </VBox>
    </center>
</BorderPane>
